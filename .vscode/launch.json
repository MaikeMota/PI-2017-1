{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Debug",
            "type": "node",
            "request": "launch",
            "cwd": "${workspaceRoot}/BACK-END/ts-node/bin",
            "program": "${cwd}/npm start",
            "stopOnEntry": false,
            "sourceMaps": true,
            "runtimeArgs": [
                "--no-timeouts",
                "--colors",
                "--compilers",
                "ts:ts-node/register"
            ]
        },
        {
            "name": "Attach",
            "type": "node",
            "request": "attach",
            "port": 9229,
            "stopOnEntry": false
        }
    ]
}